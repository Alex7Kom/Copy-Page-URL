{
  "name": "Copy Page URL",
  "description" : "Copy Page URL from context menu or toolbar",
  "version": "0.3",
  "permissions": [
    "background",
    "clipboardWrite",
    "contextMenus",
    "storage"
  ],
  "icons": {
    "16": "copy_black_16.png",
    "128": "copy_black_128.png"
  },
  "browser_action": {},
  "background": {
    "scripts": [
      "defaults.js",
      "bg.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "manifest_version": 2
}